{
  "name": "kijiji",
  "version": "1.0.1",
  "description": "Kijiji Scraper",
  "main": "index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register server/index.ts",
    "build": "ts-node -r tsconfig-paths/register build.ts && tsc",
    "dev": "nodemon -r tsconfig-paths/register server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r tsconfig-paths/register --inspect-brk' server/index.ts | pino-pretty",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit",
    "postbuild": "ef-tspm"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "cookie-parser": "^1.4.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "kijiji-scraper": "^6.1.3",
    "lodash": "^4.17.21",
    "node-json-db": "^1.3.0",
    "pino": "^6.11.2",
    "pushbullet": "^2.4.0",
    "reflect-metadata": "^0.1.13",
    "swagger-express-middleware": "^4.0.2"
  },
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/bluebird": "^3.5.24",
    "@types/chai": "^4.1.4",
    "@types/cron": "^1.6.0",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.121",
    "@types/mocha": "^5.2.5",
    "@types/pino": "^6.3.6",
    "chai": "^4.1.2",
    "mocha": "^6.0.0",
    "nodemon": "^1.18.9",
    "pino-pretty": "^2.5.0",
    "shelljs": "^0.8.5",
    "supertest": "^3.3.0",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.2.2"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
